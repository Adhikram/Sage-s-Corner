<% provide(:title, 'All users') %> 

<div class="total_head">
  <div class = "space"></div>
  <div class="space1">All users</div> 
  <div class = "search_form ">
  <%= form_tag("/users", :method => "get", class: 'd-flex search_area') do %>
    <%= text_field_tag(:search ) %>
    <%= submit_tag("Search", id:"submit") %>
  <% end %>
  </div>
</div>

<ul class="users"> 
	<%= render @users %> 
</ul>
